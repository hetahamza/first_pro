        #md_bg_color:app.theme_cls.primary_color
        on_release:
            root.manager.current="hi"
            root.manager.transition.direction='left'
<InsertData>:

    BoxLayout:
        orientation:"vertical"
        Image:
            source:"record.png"
        TextInput:
            id:bn_number
            hint_text:"Bon NUmber"


        Spinner:
            id:spinner_id1
            text:"Location"
            background_color:205/255,92/255,92/255,1
            values:['MEGWRAT123','bashyer 9','bashyer 8','bashyer 7','bashyer 6','1000 FADAN',]
            on_text:app.spinner_clicked(spinner_id1)
        TextInput:
            id:BL_NUM
            hint_text:"BLOCK NUmber"
        Spinner:
            id:spinner_id2
            text:"Structure"
            background_color:190/255,80/255,80/255,1
            values:['khawzeeq','A3MEDA','SHOKHSHEKHA','LEBSHA','SAQF','3ADYA','DAKA','DEFRA']
            on_text:app.spinner_clicked(spinner_id2)

        TextInput:
            id:qu_id
            multiline:"False"
            hint_text:"Quantity"

        Spinner:
            id:spinner_id3
            text:"Content"
            background_color:205/255,92/255,92/255,1
            values:['400','350','250','500','450','200']
            on_text:app.spinner_clicked(spinner_id3)

        Spinner:
            id:spinner_id4
            text:"Operator"
            values:['FARAHAT','ESALM','OPERATOR1','OPERATOR2','OPERATOR3']
            background_color:205/255,92/255,92/255,1
            on_text:app.spinner_clicked(spinner_id4)

        Spinner:
            id:spinner_id5
            text:"DRIVER"
            values:['M-3ABD EL-MO3Z"BONIAN"','AMR-ELFIQY"BONIAN"','RAMADAN"BONIAN"','REDA-EZZAT"BONIAN"','F-SOBHY"BONIAN"','M-MEDAHT"BONIAN"','M.SALAH"SUPPLIER"','A.GENEDY"SUPPLIER"','3ABD-EL-SAMAD"SUPPLIER"']
            background_color:205/255,92/255,92/255,1
            on_text:app.spinner_clicked(spinner_id5)

        Spinner:
            id:spinner_id6
            text:"TRUCK"
            md_bg_color:[5,2,9,1]
            values:["5238","5239","5241","5242","1596","1742","1111","2222","3333"]
            background_color:205/255,92/255,92/255,1
            on_text:app.spinner_clicked(spinner_id6)
        Spinner:
            id:spinner_id7
            text:"PUMP"
            values:['MEZRAB','BONIAN 52','BONIAN 42','PUMP GM','PUMP NASCO','PUMP BYRAMIDS','PUMP BLUE','BUMP MAKA','PUMP GOLD','RENT 1','RENT 2','RENT 2']
            background_color:205/255,92/255,92/255,1
            on_text:app.spinner_clicked(spinner_id7)
        Spinner:
            id:spinner_id8
            text:"Execution"
            values:['A.ZAKRIAA','M.SALAH','FARAHAT','ESALM','ENG HOSSAM','GAMIEL BASHA','OTHER']
            background_color:205/255,92/255,92/255,1
            on_text:app.spinner_clicked(spinner_id8)

        MDRectangleFlatIconButton:
            text:"INSERT"
            background_color:100/255,180/255,92/255,1
            icon:'bonian.png'
            md_bg_color:[5,2,9,1]
            pos_hint:{'center_x':.5,'center_y':.8}
            on_press:root.insert_data()


        MDRectangleFlatIconButton:
            text:"BACK"
            icon:'bonian.png'
            background_color:100/255,180/255,92/255,1
            md_bg_color:[5,2,9,1]
            pos_hint:{'center_x':.5,'center_y':.8}
            on_release:
                root.manager.current='hi'
                root.manager.transition.direction='right'

